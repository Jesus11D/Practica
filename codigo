numeros = [1, 2, 3, 4, 5]
print(numeros)
for n in numeros:
    if n == 3:
        print(¨Encontrado:¨, n)
izq, der = 0, len(numeros)-1
while izq <= der;
   mid = (izq + der) // 2
   if numeros[mid] == 3:
       print(¨Binaria:¨; numeros[mid])
       break
   elif numeros[mid] < 3:
       izq = mid + 1
   else:
       der = mid - 1
